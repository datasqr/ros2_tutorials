<?xml version="1.0" ?>
<robot name="My robot">
    <webots>
        <device reference="ds01" type="DistanceSensor">
            <ros>
                <topicName>/left_sensor1</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <device reference="ds11" type="DistanceSensor">
            <ros>
                <topicName>/right_sensor1</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <plugin type="my_robot_driver1::MyRobotDriver1" />
    </webots>
</robot>